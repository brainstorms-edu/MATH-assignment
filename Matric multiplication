#include <iostream>
using namespace std;

int main()
{
    int n, m, p;

    cout << "Enter rows of Matrix A: ";
    cin >> n;
    cout << "Enter columns of Matrix A (and rows of Matrix B): ";
    cin >> m;
    cout << "Enter columns of Matrix B: ";
    cin >> p;

    int a[n][m], b[m][p], c[n][p];

    cout << "\nEnter elements of Matrix A (" << n << "x" << m << "):\n";
    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            cin >> a[i][j];

    cout << "\nEnter elements of Matrix B (" << m << "x" << p << "):\n";
    for (int i = 0; i < m; i++)
        for (int j = 0; j < p; j++)
            cin >> b[i][j];


    for (int i = 0; i < n; i++)
        for (int j = 0; j < p; j++)
            c[i][j] = 0;

   
    for (int i = 0; i < n; i++)
        for (int j = 0; j < p; j++)
            for (int k = 0; k < m; k++)
                c[i][j] += a[i][k] * b[k][j];

    cout << "\nResultant Matrix (A Ã— B):\n";
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < p; j++)
            cout << c[i][j] << " ";
        cout << endl;
    }

    return 0;
}
